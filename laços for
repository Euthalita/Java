public class Main
{
	public static void main(String[] args) {
	    int x, result;
		for (int i=0; i<=10; i++){
		    result = 2*i;
		    System.out.println("2 x "+i+" = "+result);
		}
		//Para fazer laços, não dentro de outros, não pode usar a mesma letra 
		for(int j=0; j<=10; j++){
		    for(int k=0; k<=5; k++){
		        System.out.println(j+"x"+k+"="+j*k);
		    }
		}
	}
}
