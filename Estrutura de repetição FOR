import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		int valor, soma = 0;
		Scanner ler = new Scanner(System.in);
		
		for (int i=0; i<3; i++){
		    System.out.println("Insira um valor:");
		    valor = ler.nextInt();
		    soma += valor;
	    }
		System.out.println("A soma total Ã© "+soma);
	}

}
